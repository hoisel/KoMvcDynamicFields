@model string

<div class="dynamic-item">
    <div class="form-group">
        @Html.Label("Nickname")
        @Html.TextBoxFor(model => model, new { data_bind = "value: $data, dynamicName: $parent.Nicknames().length", @class = "form-control" })
    </div>
    <div class="delete-group">
        <span data-bind="click: $parent.removeNickname" class="glyphicon glyphicon-remove-circle btn-delete" title="remove this item"></span>
    </div>
</div>

